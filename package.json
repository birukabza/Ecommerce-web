{
  "name": "any-store",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "20.15.1",
    "npm": "10.7.0"
  },
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "compression": "1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "stripe": "^17.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
